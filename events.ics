{%- capture newLine -%}
{%- endcapture -%}
BEGIN:VCALENDAR
VERSION:2.0
PRODID:http://www.example.com/
METHOD:PUBLISH
{{newLine}}
{%- for event in site.events -%}
BEGIN:VEVENT
UID:{{ event.start_date | date: "%Y%m%d%H%M%S" }}@gregariousmammal.com
