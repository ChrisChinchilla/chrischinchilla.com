---
layout: null
---

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <ttl>30</ttl>
      {% for post in site.data.ethicsposts.list limit:10 %}
    <item>
      <title>{{ post[1].resolved_title | xml_escape }} from {{post[1].domain_metadata.name}}</title>
      <link>{{ post[1].resolved_url }}</link>
      <pubDate>{{ post[1].time_added | date: "%Y-%m-%dT%H:%M:%SZ" }}</pubDate>
      <description>{{ post[1].excerpt | markdownify }}</description>
      <guid>urn:uuid:{{post[1].item_id}}</guid>
    </item>
      {% endfor %}
  </channel>
</rss>