---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title type="text" xml:lang="en">Latest Ethics News</title>
    <link type="application/atom+xml" href="{{ site.baseurl }}/atom.xml" rel="self"/>
    <link href="{{ site.baseurl }}/"/>
  <updated>{{ site.time | date: "%Y-%m-%dT%H:%M:%SZ" }}</updated>
  <id>{{ site.baseurl }}/</id>
  <author>
    <name>{{ site.title }}</name>
    <email>{{ site.email }}</email>
  </author>
  <rights type="text">Copyright Â© {{ site.time | date: "%Y" }} {{ site.author }}. All rights reserved.</rights>
  {% for post in site.data.ethicsposts.list limit:10 %}
  <entry>
    <title>{{ post[1].resolved_title | xml_escape }} from {{post[1].domain_metadata.name}}</title>
    <link rel="alternate" type="text/html" href="{{ site.baseurl }}{{ post.url }}/?utm_source=RSS&amp;utm_medium=RSS&amp;utm_campaign={{ post.title | replace: ',','' | replace: ' ', '' | xml_escape }}" />
    <published>{{ post[1].time_added | date: "%Y-%m-%dT%H:%M:%SZ" }}</published>
    <updated>{{ post[1].time_added | date_to_xmlschema }}</updated>
    <id>{{ site.baseurl }}{{ post[1].item_id }}</id>
    <content type="html"><![CDATA[ {{ post[1].excerpt | markdownify }} ]]></content>
  </entry>
  {% endfor %}

</feed>